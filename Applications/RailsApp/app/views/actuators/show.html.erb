<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @actuator.name %>
</p>
<p>
	<strong>Flavors of this actuator:</strong>
	<ul>
	<% @actuator.flavors.each do |flavor| %>
		<li><%= link_to flavor.name.humanize, flavor %></li>
	<% end %>
	</ul>
</p>
<p>
	<strong>Behaviors usable with this actuator:</strong>
	<ul>
	<% @actuator.behaviors.each do |behavior| %>
		<li><%= link_to behavior.name.humanize, behavior %></li>
	<% end %>
	</ul>
</p>

<p>
	<strong>Experimental Data:</strong>
	<%= @actuator.experiment %>
</p>

<%= link_to 'Edit', edit_actuator_path(@actuator) %> |
<%= link_to 'Back', actuators_path %>
